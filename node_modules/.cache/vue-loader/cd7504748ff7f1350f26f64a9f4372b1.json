              this.post = reponse.data.note;\n                console.log(this.post)\n            });\n    },\n    methods: {\n        deletePost() {\n            const deleted = {\n                method: 'DELETE',\n                headers: {\n                    \"Content-Type\": \"application/json\"\n                }\n            };\n            fetch(\"http://5.135.119.239:3090/notes/\" + this.$route.params._id, deleted)\n            this.$router.push('/');\n        }\n    }\n}\n</script>"]}]}