te.vue","sourceRoot":"","sourcesContent":["<template>\n<div class=\"container\">\n    <h2>Create your Postit</h2>\n    <router-link to =\"/\">Home</router-link>\n    <div class=\"c-note\">\n        <input  v-model=\"title\" class=\"note\" placeholder=\"postit title\"> \n       <textarea cols=\"30\" rows=\"10\" v-model=\"text\" class=\"note\"></textarea>\n       <button class=\"note\" type=\"submit\">create</button>\n    </div>\n</div>\n    \n</template>\n<script>\n    export default {\n        data() {\n            return {\n                postit: {}\n            }\n        },\n        methods: {\n            verifPostit(e) {\n                e.preventDefault()\n                let postitActuel = JSON.parse(localStorage.getItem('postits'))\n\n                const newPostit = {\n                    title: this.title,\n                    text: this.text,\n                }\n\n                if(!postitActuel || postitActuel.length == 0) {\n                    const postits = [{...newPostit, id: 1}]\n                    localStorage.setItem('postits', JSON.stringify(postits))\n                }else{\n                    let ids = postitActuel.map( postit => postit.id)\n                    let id = Math.max(...ids) + 1\n\n                    const toSend = postitActuel.concat({...newPostit, id})\n                    localStorage.setItem('postits', JSON.stringify(toSend))\n                }\n                this.$router.back()\n            }\n        }\n    }\n</script>\n<style>\n.container{\n    height: 100vh;\n    margin: 0 auto;\n}\n.c-note{\n    margin: 0 auto;\n    margin-top : 8vh;\n    height: 50vh;\n    display:flex;\n    flex-direction: column;\n    justify-content: center;\n    width: 50%;\n}\n.note{\n    margin-top:7vh;\n    border: 4px solid black;\n}\n</style>\n"]}]}