 :key=\"post\" class=\"post\">\n      <div class=\"title\">\n        <h3>{{ truncate(post.title, 20) }}</h3>\n      </div>\n      <p v-for=\"content in post.content\" :key=\"content\">\n        {{ truncate(content, 200) }}\n      </p>\n      <router-link :to=\"{ name: 'note', params: { _id: post._id } }\">\n        <img src=\"../assets/fleche.png\" alt=\"\" /></router-link>\n    </div>\n  </div>\n  <pagination v-model=\"currentPage\" :total-rows=\"rows\" :per-page=\"perPage\" aria-controls=\"my-table\">\n  </pagination>\n  <p class=\"mt-3\">Current Page: {{ currentPage }}</p>\n</template>\n<script>\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      perPage: 6,\n      currentPage: 1,\n      posts: [],\n    };\n  },\n  mounted() {\n    const headers = { \"Content-Type\": \"application/json\" };\n    axios\n      .get(\"https://post-it-7m1i.onrender.com/notes/\", { headers })\n      .then((reponse) => {\n        this.posts = reponse.data.notes;\n        console.log(this.posts);\n      });\n  },\n  computed: {\n    rows() {\n      return this.posts.length\n    }\n  },\n  updated() {\n    const headers = { \"Content-Type\": \"application/json\" };\n    axios\n      .get(\"https://post-it-7m1i.onrender.com/notes/\", { headers })\n      .then((reponse) => {\n        this.posts = reponse.data.notes;\n      });\n  },\n  methods: {\n    truncate(string, len) {\n      if (string.length > len) {\n        string = string.substring(0, len);\n        return string + \"...\";\n      } else\n        return string;\n    },\n  }\n};\n</script>\n<style>\n.posts {\n  padding-top: 10vh;\n  display: grid;\n  margin: 0 auto;\n  gap: 1rem;\n}\n\n.posts :nth-child(3n) {\n  background-color: bisque;\n}\n\n.posts :nth-child(4n) {\n  background-color: azure;\n}\n\n.post {\n  padding: 1rem;\n  background-color: rgb(240, 230, 140);\n  box-shadow: 0px 10px 10px 10px grey;\n  height: 35vh;\n  margin: 10%;\n  word-wrap: break-word;\n}\n\n/*RESPONSIVE*/\n@media (min-width: 600px) {\n  .posts {\n    grid-template-columns: repeat(2, 1fr);\n  }\n}\n\n@media (min-width: 950px) {\n  .posts {\n    grid-template-columns: repeat(3, 1fr);\n  }\n}\n</style>"]}]}