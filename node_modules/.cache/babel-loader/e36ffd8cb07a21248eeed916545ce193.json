s.post(\"https://post-it-7m1i.onrender.com/notes/\", posts, {headers})\n            .then(response => this.note_id = response.data.note_id);\n            this.$router.push('/');\n        }\n        \n    }\n    \n   }\n</script>\n<style>\n.container{\n    height: 100vh;\n    margin: 0 auto;\n}\n.c-note{\n    margin: 0 auto;\n    margin-top : 8vh;\n    height: 50vh;\n    display:flex;\n    flex-direction: column;\n    justify-content: center;\n    width: 50%;\n}\n.note{\n    margin-top:7vh;\n    border: 4px solid black;\n}\n</style>\n"]}]}